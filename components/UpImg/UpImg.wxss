.up_img_wrap {
    margin-top: 20rpx;
    margin-left: 10rpx;
    width: 120rpx;
    height: 90rpx;
    position: relative;
}

.up_img_wrap image {
    width: 100%;
    height: 100%;
    border-radius: 15rpx;
    padding: 0 10rpx 0 20rpx;
}

.up_img_wrap icon {
    position: absolute;
    top: -28rpx;
    right: -10rpx;
}